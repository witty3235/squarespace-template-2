  <script src="//ajax.googleapis.com/ajax/libs/jquery/2.0.0/jquery.min.js"></script>

  <!--Combo and minify the scripts when you're not logged in.-->
  <squarespace:script src="site.js" combo="{.if authenticatedAccount}false{.or}true{.end}" />

  <!--Injection point for tracking scripts and user content from the code injection tab.-->
  {squarespace-footers}

  <script>
    var cvgSplashShift = document.querySelector("#cvg-splash-shift");
    cvgSplashShift.style.height = window.innerHeight + "px";

    var subsequentSplashes = document.querySelectorAll(".cvg-subsequent-splash");
    for (var i = 0; i < subsequentSplashes.length; i++) {
      subsequentSplashes[i].style.height = window.innerHeight + "px";
      subsequentSplashes[i].style.width = window.innerWidth + "px";
    }
  </script>

  </body>
</html> 
